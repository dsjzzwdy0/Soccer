function handler(event) {
    $("#allList").addClass("cur").siblings(".more2").removeClass("cur");
    var i = 0;
    if (this.value == '10') {
        $("tbody").attr("style", "display:black");
        for (i = 0; i <= 29; i++) {
            if (i < 10) {
                $("#m_" + i).show();
            } else {
                $("#m_" + i).hide();
            }
        }
    } else if (this.value == '20') {
        $("tbody").attr("style", "display:black");
        for (i = 0; i <= 29; i++) {
            if (i < 20) {
                $("#m_" + i).show();
            } else {
                $("#m_" + i).hide();
            }
        }
    } else {
        $("tbody").attr("style", "display:black");
        for (i = 0; i <= 29; i++) {
            $("#m_" + i).show();
        }
    }
}
$("#gender li").click(handler);

